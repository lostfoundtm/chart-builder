      <HBox>
         <children>
            <VBox fx:id="chartBox" prefHeight="400.0" prefWidth="600.0" />
            <VBox>
               <children>
                  <HBox>
                     <children>
                        <Label text="x0" />
                        <TextField text="0" fx:id="x0">
                           <HBox.margin>
                              <Insets left="10.0" />
